<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kannadasang.androidactionbar">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.kannadasang.androidactionbar.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.kannadasang.androidactionbar.AboutActivity"
            android:label="About Us" />
        <activity
            android:name="com.example.kannadasang.androidactionbar.ContactActivity"
            android:label="Contact Us"></activity>
    </application>

</manifest>